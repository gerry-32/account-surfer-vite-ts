{
  "appId": "dospolov.com.AccountSurfer",
  "productName": "AccountSurfer",
  "directories": {
    "output": "dist"
  },
  "files": [
    "main",
    "src/out",
    "static"
  ],
  "win": {
    "target": "nsis"
  },
  "extraResources": [
    {
      "from": "node_modules/regedit/vbs",
      "to": "regedit/vbs"
    }
  ],
  "nsis": {
    "perMachine": true,
    "allowElevation": true,
    "runAfterFinish": true,
    "include": "buildResources/installer.nsh"
  },
  "publish": null
}
